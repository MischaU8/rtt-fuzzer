    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Fuzz Inferno",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "rtt-module"
            ],
            "program": "${workspaceRoot}/node_modules/@jazzer.js/core/dist/cli.js",
            "env": {
                "RTT_RULES": "../server/public/inferno/rules.js",
                "RND": "true",
                "NO_UNDO": "true",
                "NO_RESIGN" : "true"
              }
        }
    ]
}